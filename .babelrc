{
  "presets": [
    ["es2015", {"modules": false}], "react", "stage-0"
  ],

  "plugins": [
    "react-hot-loader/babel"
  ],

  "env": {
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {
          "mode": "wrap",
          "ignoreFilenames": ["node_modules"]
        }]
      ]
    }
  }

}